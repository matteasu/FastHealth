<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FastHealth" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2196F3</item>
        <item name="colorPrimaryVariant">#1976D2</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="backgroundColor">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#609CCD</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

    </style>

    <style name="Widget.App.Chip" parent="Widget.MaterialComponents.Chip.Entry">
        <item name="chipBackgroundColor">#782196F3</item>
        <item name="android:fontFamily">sans-serif-black</item>
        <item name="chipStrokeColor">#782A7ED2</item>
        <item name="checkedIconVisible">false</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">true</item>
    </style>

    <style name="BottoniRossi" parent="Widget.MaterialComponents.Button">
        <item name="colorButtonNormal">#8FFF0000</item>

    </style>
    <style name="CustomDialog" parent="Theme.MaterialComponents.Dialog" >
        <item name="android:windowIsFloating">true</item>
    </style>
</resources>